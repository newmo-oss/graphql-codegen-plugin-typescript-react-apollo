{
  "name": "@newmo/graphql-codegen-plugin-typescript-react-apollo",
  "version": "1.0.1",
  "description": "GraphQL Codegen Plugin to create React Hooks for client preset",
  "keywords": [
    "graphql",
    "plugin",
    "react",
    "hook"
  ],
  "homepage": "https://github.com/newmo-oss/graphql-codegen-plugin-typescript-react-apollo",
  "bugs": {
    "url": "https://github.com/newmo-oss/graphql-codegen-plugin-typescript-react-apollo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newmo-oss/graphql-codegen-plugin-typescript-react-apollo.git"
  },
  "license": "MIT",
  "author": "newmo, Inc.",
  "type": "commonjs",
  "main": "lib/graphql-codegen-plugin-typescript-react-apollo.js",
  "files": [
    "lib/"
  ],
  "workspaces": [
    "./test"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepare": "git config --local core.hooksPath .githooks",
    "updateSnapshot": "npm run build && graphql-codegen --config ./test/graphql-codegen.ts",
    "test": "npm run updateSnapshot && cd test && npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@types/node": "^20.11.29",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "@mizdra/graphql-codegen-typescript-fabbrica": "^0.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
